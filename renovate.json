{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>aquaproj/aqua-renovate-config#1.2.6",
    "github>aquaproj/aqua-renovate-config:installer-script#1.2.6(.circleci/config\\.yml)"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": ["aqua-registry"],
      "automerge": true
    }
  ],
  "platformAutomerge": true,
  "regexManagers": [
    {
      "fileMatch": [
        ".circleci/config.yml"
      ],
      "matchStrings": [
        "aqua-installer \\| bash -s -- -v (?<currentValue>.*)\n"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "aquaproj/aqua"
    }
  ]
}
